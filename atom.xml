    <title>上海红茶馆 ~ Chinese Tea （个人填词片段）</title>
    <link href="http://example.com/2023/05/24/%E4%B8%8A%E6%B5%B7%E7%BA%A2%E8%8C%B6%E9%A6%86%20~%20Chinese%20Tea%20%EF%BC%88%E4%B8%AA%E4%BA%BA%E5%A1%AB%E8%AF%8D%E7%89%87%E6%AE%B5%EF%BC%89/"/>
    <id>http://example.com/2023/05/24/%E4%B8%8A%E6%B5%B7%E7%BA%A2%E8%8C%B6%E9%A6%86%20~%20Chinese%20Tea%20%EF%BC%88%E4%B8%AA%E4%BA%BA%E5%A1%AB%E8%AF%8D%E7%89%87%E6%AE%B5%EF%BC%89/</id>
